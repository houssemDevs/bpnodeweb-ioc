{
  "name": "bpnodeweb",
  "version": "0.0.1",
  "description": "a nodejs web service based on koa.",
  "scripts": {
    "test": "npx jest",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack --config webpack.dev.ts",
    "prod": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" npx webpack --config webpack.prod.ts",
    "dev:run": "npx nodemon build/service.js",
    "prepare": "npm run prod",
    "preversion": "npm run prod && npm t",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/houssemdevs/bpnodeweb"
  },
  "keywords": [
    "nodejs",
    "web",
    "service",
    "koa",
    "koa-router",
    "webpack",
    "typescript",
    "jest"
  ],
  "author": "GHIAT Houssem <ghiat.houssem@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.48",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.0.40",
    "@types/node": "^11.13.0",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.27",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.10",
    "raw-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "inversify": "^5.0.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-ioc-utils": "^0.1.1",
    "koa-logger-middleware": "^1.0.0",
    "koa-router": "^7.4.0",
    "reflect-metadata": "^0.1.13",
    "stronger-typed-streams": "^0.1.2",
    "uuid": "^3.3.2"
  }
}
